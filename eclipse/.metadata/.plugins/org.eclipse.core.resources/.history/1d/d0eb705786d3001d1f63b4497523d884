package lanqiao_test;


public class eg5 {
	public static void main(String[] args) {
		int count = 0;
		String s = "";
		for(int i = 1 ; i < 2020;i ++) {
			s = i + "";
			for(int j = 0 ; j < s.length();j ++) {
				if(s.charAt(j) == '2') {
					count++;
				}
			}
		}
		System.out.print(count);
	}
}
