package lanqiao_test;


public class eg5 {
	public static void main(String[] args) {
		int[] arr= new int[9]; 
		for(int i = 0 ;i < 2019 ; i++) {
			if( i > 1000) {
				int a = i % 10;
				i /=10;
				arr[a]++;
			}if( i > 100) {
				int a = i % 10;
				i /=10;
				arr[a]++;
			}if( i >= 10) {
				int a = i % 10;
				i /=10;
				arr[a]++;
			}if(i<10) {
				int a =i;
				arr[a]++;
			}
		}
		System.out.print(arr[2]);
	}
}
